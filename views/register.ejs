	<script type="text/javascript">
    $(".username-unavailable").css("display", "none");
    $(document).ready(function() {
    $('#username').keyup(function() {
    var value = $(this).val();
    
    $.ajax({
      type: 'post',
      url: 'user_available_check.php',
      data: {'username' : value},
      success: function(r) {
        $('.username-unavailable').html(r);
        $(".username-unavailable").css("display", "");
                            }
      });
      });
      });
    </script>
	
